<script type="text/javascript">
    $(document).ready(function(){
       $(".btn").click(function(){
           $(".overlay").removeClass("close");
           $(".overlay").addClass("open");
       });
        $(".overlay-close").click(function(){
            $(".overlay").removeClass("open");
            $(".overlay").addClass("close");
       });
    });
</script>